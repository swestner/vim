             aria-valuenow="{{value}}/20" aria-valuemin="0" aria-valuemax="{{max}}" ng-style="{width: percent + '%'}"
             aria-valuetext="{{percent | number:0}}%" ng-transclude>
            <span class="hidden-accessible-label">{{value}}/20</span>
